{
  "name": "bizsquad",
  "version": "3.0.41",
  "author": {
    "name": "kimtaehwan",
    "email": "skyzard1@gmail.com",
    "url": "https://www.bizsquad.net"
  },
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "dev": "nf start",
    "start": "ionic-app-scripts serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "publish": "build --mac --win -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mw"
  },
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/compiler-cli": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/router": "^8.0.3",
    "@ionic-native/core": "~4.19.0",
    "@ionic-native/splash-screen": "~4.19.0",
    "@ionic-native/status-bar": "~4.19.0",
    "@ionic/angular": "^4.6.0",
    "@ionic/storage": "2.2.0",
    "@types/deep-equal": "^1.0.1",
    "deep-equal": "^1.0.1",
    "electron-context-menu": "^0.12.1",
    "electron-log": "^3.0.1",
    "electron-push-receiver": "^2.1.2",
    "electron-updater": "^4.0.6",
    "electron-window-state": "^5.0.3",
    "firebase": "^5.7.2",
    "ionic-angular": "3.9.2",
    "ionicons": "4.5.1",
    "jquery": "^3.3.1",
    "keycode": "^2.2.0",
    "rxjs": "6.3.3",
    "rxjs-compat": "^6.3.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.1",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "typescript": "~3.2.2"
  },
  "description": "ionic framework based electron project",
  "main": "electron/electron.js",
  "config": {
    "ionic_bundler": "webpack",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "opentask",
        "repo": "bizsquadpc"
      }
    ],
    "appId": "com.bizsquad.ionic-electron",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/logo512.png"
    },
    "win": {
      "verifyUpdateCodeSignature": false,
      "target": [
        "nsis"
      ],
      "icon": "build/logo512.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "language": 1032,
      "shortcutName": "Bizsquad"
    },
    "asar": true,
    "files": [
      "www/**/*",
      "electron/*"
    ]
  }
}
